,aAAa,mCAAmC,UAAU,gBAAA;AAAA,MAC/J,EAAE,IAAI,IAAI,KAAK,oCAAoC,KAAK,iBAAiB,OAAO,uBAAuB,aAAa,kCAAkC,UAAU,gBAAA;AAAA;AAAA,MAGhK,EAAE,IAAI,IAAI,KAAK,uCAAuC,KAAK,mBAAmB,OAAO,oBAAoB,aAAa,wCAAwC,UAAU,gBAAA;AAAA,MACxK,EAAE,IAAI,IAAI,KAAK,wCAAwC,KAAK,oBAAoB,OAAO,kBAAkB,aAAa,yCAAyC,UAAU,gBAAA;AAAA,IAAgB;AAG3L,UAAM,iBAAiB,SAAS,MAAM;AACpC,UAAI,WAAW,eAAe,UAAU,QACpC,YACA,UAAU,OAAO,CAAA,QAAO,IAAI,aAAa,eAAe,KAAK;AAEjE,aAAO,SAAS,MAAM,GAAG,aAAa,KAAK;AAAA,IAC7C,CAAC;AAED,UAAM,UAAU,SAAS,MAAM;AAC7B,YAAM,gBAAgB,eAAe,UAAU,QAC3C,UAAU,SACV,UAAU,OAAO,CAAA,QAAO,IAAI,aAAa,eAAe,KAAK,EAAE;AAEnE,aAAO,aAAa,QAAQ;AAAA,IAC9B,CAAC;AAED,UAAM,eAAe,SAAS,MAAM;AAClC,aAAO,eAAe,MAAM,kBAAkB,KAAK,KAAK,CAAA;AAAA,IAC1D,CAAC;AAED,UAAM,kBAAkB,CAAC,eAAe;AACtC,YAAM,WAAW,WAAW,KAAK,CAAA,QAAO,IAAI,OAAO,UAAU;AAC7D,aAAO,WAAW,SAAS,OAAO;AAAA,IACpC;AAEA,UAAM,eAAe,CAAC,UAAU;AAE9B,YAAM,WAAW;AAAA,QACf;AAAA,QACA;AAAA,QACA;AAAA,QACA;AAAA,QACA;AAAA,QACA;AAAA,MAAA;AAEF,aAAO,SAAS,QAAQ,SAAS,MAAM;AAAA,IACzC;AAkCA,UAAM,gBAAgB,MAAM;AAC1B,mBAAa,QAAQ;AAAA,IACvB,CAAC;;;AA3PM,YAAA,OAAAA,eAAAC,WAAA,EAAA,OAAM,WAAO,MAAA,CAAA,CAAA,4bAAA;AAcWC,oBAAA,aAAZ,aAAQ;gDAIPC,MAAA,cAAA,MAAmB,SAAS,0LAIrB,QAAM,SAAS,IAAI,kCAC/BC,eAAA,SAAS,IAAI,8EAEXA,eAAA,SAAS,KAAK;;;oBAQMD,MAAA,cAAA,GAAc,CAA/B,OAAO,UAAK;AAIZ,cAAA,eAAAE,eAAA,CAAA,aAAa,KAAK,GAAA,mHAAA,CAAA,CAAA,IAAA;;UAGvB,KAAK,MAAM;AAAA,UACX,KAAK,MAAM;AAAA,UACZ,OAAM;AAAA,UACN,SAAQ;AAAA,QAAA;gNAMH,gBAAgB,MAAM,QAAQ,CAAA,CAAA,qBAAA;AAAA;;UAO9BF,MAAA,OAAA,GAAO;iGAIHA,MAAA,OAAA,CAAO,IAAA,cAAA,EAAA,GAAA;YAGVA,MAAA,OAAA,GAAO;;;;;iCAIZA,MAAA,OAAA,IAAO,eAAA,kBAAA,CAAA,iBAAA;AAAA;;;UAMNA,MAAA,YAAA,GAAY;;YAcVA,MAAA,iBAAA,IAAiB,GAAA;;;;;YASjBA,MAAA,iBAAA,IAAoBA,MAAA,cAAA,EAAe,SAAM,GAAA;;;;;;;UAS5C,KAAKA,MAAA,YAAA,EAAa;AAAA,UAClB,KAAKA,MAAA,YAAA,EAAa;AAAA,UACnB,OAAM;AAAA,QAAA;6HAIoCC,eAAAD,MAAA,YAAA,EAAa,KAAK,0CACxBC,eAAAD,MAAA,YAAA,EAAa,WAAW,+DAEvDA,MAAA,iBAAA,IAAiB,CAAA,QAAcC,eAAAD,MAAA,cAAA,EAAe,MAAM;;;;;;;;;;;;;;","names":["_ssrRenderAttrs","_mergeProps","_ssrRenderList","_unref","_ssrInterpolate","_ssrRenderClass"],"sources":["../../../../../../../pages/gallery.vue"],"sourcesContent":["<template>\n  <div class=\"pt-16\">\n    <div class=\"section-padding\">\n      <div class=\"container-max\">\n        <div class=\"text-center mb-16\">\n          <h1 class=\"text-4xl md:text-5xl font-bold mb-6\">Photo Gallery</h1>\n          <p class=\"text-xl text-gray-600 max-w-3xl mx-auto\">\n            Explore our visual journey through photographs capturing the incredible moments, \n            landscapes, and experiences from our record-breaking circumnavigation adventures.\n          </p>\n        </div>\n\n        <!-- Category Filter -->\n        <div class=\"flex flex-wrap justify-center gap-4 mb-12\">\n          <button \n            v-for=\"category in categories\" \n            :key=\"category.id\"\n            @click=\"activeCategory = category.id\"\n            class=\"px-6 py-2 rounded-full transition-colors duration-200 flex items-center\"\n            :class=\"activeCategory === category.id \n              ? 'bg-blue-600 text-white' \n              : 'bg-gray-100 text-gray-700 hover:bg-gray-200'\"\n          >\n            <iconify-icon :icon=\"category.icon\" class=\"mr-2\"></iconify-icon>\n            {{ category.name }}\n            <span class=\"ml-2 bg-white bg-opacity-20 px-2 py-1 rounded-full text-xs\">\n              {{ category.count }}\n            </span>\n          </button>\n        </div>\n\n        <!-- Gallery Grid -->\n        <div class=\"grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 lg:grid-cols-4 gap-4\">\n          <div \n            v-for=\"(image, index) in filteredImages\" \n            :key=\"image.id\"\n            @click=\"openLightbox(index)\"\n            class=\"relative group cursor-pointer overflow-hidden rounded-lg shadow-lg hover:shadow-xl transition-shadow duration-300\"\n            :class=\"getGridClass(index)\"\n          >\n            <NuxtImg \n              :src=\"image.src\" \n              :alt=\"image.alt\"\n              class=\"w-full h-full object-cover group-hover:scale-105 transition-transform duration-300\"\n              loading=\"lazy\"\n            />\n            \n            <!-- Category badge -->\n            <div class=\"absolute top-2 left-2 opacity-0 group-hover:opacity-100 transition-opacity duration-300\">\n              <span class=\"bg-white bg-opacity-90 px-2 py-1 rounded text-xs font-medium\">\n                {{ getCategoryName(image.category) }}\n              </span>\n            </div>\n          </div>\n        </div>\n\n        <!-- Load More -->\n        <div v-if=\"hasMore\" class=\"text-center mt-12\">\n          <button \n            @click=\"loadMore\"\n            class=\"btn-primary\"\n            :disabled=\"loading\"\n          >\n            <iconify-icon \n              v-if=\"loading\" \n              icon=\"lucide:loader-2\" \n              class=\"mr-2 animate-spin\"\n            ></iconify-icon>\n            {{ loading ? 'Loading...' : 'Load More Photos' }}\n          </button>\n        </div>\n\n        <!-- Lightbox -->\n        <div \n          v-if=\"lightboxOpen\" \n          @click=\"closeLightbox\"\n          class=\"fixed inset-0 bg-black bg-opacity-90 z-50 flex items-center justify-center p-4\"\n        >\n          <!-- Close button - top right of screen -->\n          <button \n            @click=\"closeLightbox\"\n            class=\"fixed top-4 right-4 text-white hover:text-gray-300 z-20 bg-black bg-opacity-50 rounded-full p-2\"\n          >\n            <iconify-icon icon=\"lucide:x\" class=\"text-2xl\"></iconify-icon>\n          </button>\n          \n          <!-- Previous button - left edge of screen -->\n          <button \n            v-if=\"currentImageIndex > 0\"\n            @click.stop=\"previousImage\"\n            class=\"fixed left-2 sm:left-4 top-1/2 transform -translate-y-1/2 text-white hover:text-gray-300 z-10 bg-black bg-opacity-50 rounded-full p-2 sm:p-3\"\n          >\n            <iconify-icon icon=\"lucide:chevron-left\" class=\"text-2xl sm:text-4xl\"></iconify-icon>\n          </button>\n          \n          <!-- Next button - right edge of screen -->\n          <button \n            v-if=\"currentImageIndex < filteredImages.length - 1\"\n            @click.stop=\"nextImage\"\n            class=\"fixed right-2 sm:right-4 top-1/2 transform -translate-y-1/2 text-white hover:text-gray-300 z-10 bg-black bg-opacity-50 rounded-full p-2 sm:p-3\"\n          >\n            <iconify-icon icon=\"lucide:chevron-right\" class=\"text-2xl sm:text-4xl\"></iconify-icon>\n          </button>\n\n          <div class=\"relative max-w-4xl max-h-full\">\n            <NuxtImg \n              :src=\"currentImage.src\" \n              :alt=\"currentImage.alt\"\n              class=\"max-w-full max-h-full object-contain\"\n            />\n            \n            <div class=\"absolute bottom-4 left-4 right-4 text-white text-center\">\n              <h3 class=\"text-lg font-semibold mb-1\">{{ currentImage.title }}</h3>\n              <p class=\"text-sm text-gray-300\">{{ currentImage.description }}</p>\n              <div class=\"mt-2 text-xs text-gray-400\">\n                {{ currentImageIndex + 1 }} of {{ filteredImages.length }}\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nuseSeoMeta({\n  title: 'Gallery - Saloo & Neena Chowdhury Photo Collection',\n  description: 'Browse through our extensive photo gallery capturing moments from record-breaking circumnavigation journeys across six continents.',\n  ogTitle: 'Gallery - Saloo & Neena Chowdhury Photo Collection'\n})\n\nconst activeCategory = ref('all')\nconst lightboxOpen = ref(false)\nconst currentImageIndex = ref(0)\nconst displayCount = ref(20)\nconst loading = ref(false)\n\nconst categories = [\n  { id: 'all', name: 'All Photos', icon: 'lucide:image', count: 45 },\n  { id: 'journey', name: 'Journey Moments', icon: 'lucide:car', count: 15 },\n  { id: 'landscapes', name: 'Landscapes', icon: 'lucide:mountain', count: 12 },\n  { id: 'people', name: 'People & Culture', icon: 'lucide:users', count: 8 },\n  { id: 'achievements', name: 'Achievements', icon: 'lucide:trophy', count: 6 },\n  { id: 'behind-scenes', name: 'Behind the Scenes', icon: 'lucide:camera', count: 4 }\n]\n\nconst allImages = [\n  // Journey Moments\n  { id: 1, src: '/images/gallery/Scan1.jpg', alt: 'Journey Start', title: 'The Beginning', description: 'Starting our historic journey from Delhi', category: 'journey' },\n  { id: 2, src: '/images/gallery/Scan2.jpg', alt: 'Car Preparation', title: 'Getting Ready', description: 'Final preparations before departure', category: 'journey' },\n  { id: 3, src: '/images/gallery/Scan3.jpg', alt: 'Border Crossing', title: 'International Borders', description: 'Crossing international boundaries', category: 'journey' },\n  { id: 4, src: '/images/gallery/Scan4.jpg', alt: 'Journey Moments', title: 'On the Road', description: 'Memorable moments during the journey', category: 'journey' },\n  { id: 5, src: '/images/gallery/Scan5.jpg', alt: 'Adventure', title: 'Adventure Time', description: 'Exciting moments from the expedition', category: 'journey' },\n  \n  // Landscapes\n  { id: 6, src: '/images/gallery/Scan6.jpg', alt: 'Scenic Route', title: 'Beautiful Landscapes', description: 'Stunning views encountered during travel', category: 'landscapes' },\n  { id: 7, src: '/images/gallery/Scan7.jpg', alt: 'Mountain Roads', title: 'Mountain Passes', description: 'Challenging mountain terrain', category: 'landscapes' },\n  { id: 8, src: '/images/gallery/Scan8.jpg', alt: 'Desert Crossing', title: 'Desert Adventures', description: 'Crossing vast desert landscapes', category: 'landscapes' },\n  { id: 9, src: '/images/gallery/Scan9.jpg', alt: 'Coastal Views', title: 'Coastal Journey', description: 'Beautiful coastal roads and views', category: 'landscapes' },\n  \n  // People & Culture\n  { id: 10, src: '/images/gallery/Scan10.jpg', alt: 'Local People', title: 'Meeting Locals', description: 'Connecting with people from different cultures', category: 'people' },\n  { id: 11, src: '/images/gallery/Scan11.jpg', alt: 'Cultural Exchange', title: 'Cultural Moments', description: 'Learning about local traditions', category: 'people' },\n  { id: 12, src: '/images/gallery/Scan12.jpg', alt: 'Team Photo', title: 'Team Moments', description: 'Special moments with the team', category: 'people' },\n  \n  // Achievements\n  { id: 13, src: '/images/gallery/Scan13.jpg', alt: 'Achievement', title: 'World Record Achievement', description: 'Celebrating the world record', category: 'achievements' },\n  { id: 14, src: '/images/gallery/Scan14.jpg', alt: 'Recognition', title: 'Official Recognition', description: 'Receiving official recognition', category: 'achievements' },\n  \n  // Behind the Scenes\n  { id: 15, src: '/images/gallery/Scan-006.BMP.jpg', alt: 'Behind Scenes', title: 'Behind the Scenes', description: 'Candid moments from the journey', category: 'behind-scenes' },\n  { id: 16, src: '/images/gallery/Scan-007.BMP.jpg', alt: 'Preparation', title: 'Journey Preparation', description: 'Getting ready for the adventure', category: 'behind-scenes' },\n  { id: 17, src: '/images/gallery/Scan-008.BMP.jpg', alt: 'Documentation', title: 'Documenting History', description: 'Recording the historic journey', category: 'behind-scenes' },\n  \n  // Behind the Scenes\n  { id: 11, src: '/images/gallery/mechanical-work.jpg', alt: 'Mechanical Work', title: 'Roadside Repairs', description: 'Fixing mechanical issues on the road', category: 'behind-scenes' },\n  { id: 12, src: '/images/gallery/planning-session.jpg', alt: 'Planning Session', title: 'Route Planning', description: 'Strategic planning during the journey', category: 'behind-scenes' }\n]\n\nconst filteredImages = computed(() => {\n  let filtered = activeCategory.value === 'all' \n    ? allImages \n    : allImages.filter(img => img.category === activeCategory.value)\n  \n  return filtered.slice(0, displayCount.value)\n})\n\nconst hasMore = computed(() => {\n  const totalFiltered = activeCategory.value === 'all' \n    ? allImages.length \n    : allImages.filter(img => img.category === activeCategory.value).length\n  \n  return displayCount.value < totalFiltered\n})\n\nconst currentImage = computed(() => {\n  return filteredImages.value[currentImageIndex.value] || {}\n})\n\nconst getCategoryName = (categoryId) => {\n  const category = categories.find(cat => cat.id === categoryId)\n  return category ? category.name : ''\n}\n\nconst getGridClass = (index) => {\n  // Create varied grid layout\n  const patterns = [\n    'col-span-1 row-span-1',\n    'col-span-1 row-span-1', \n    'col-span-1 row-span-2',\n    'col-span-1 row-span-1',\n    'col-span-2 row-span-1',\n    'col-span-1 row-span-1'\n  ]\n  return patterns[index % patterns.length]\n}\n\nconst openLightbox = (index) => {\n  currentImageIndex.value = index\n  lightboxOpen.value = true\n  document.body.style.overflow = 'hidden'\n}\n\nconst closeLightbox = () => {\n  lightboxOpen.value = false\n  document.body.style.overflow = 'auto'\n}\n\nconst nextImage = () => {\n  if (currentImageIndex.value < filteredImages.value.length - 1) {\n    currentImageIndex.value++\n  }\n}\n\nconst previousImage = () => {\n  if (currentImageIndex.value > 0) {\n    currentImageIndex.value--\n  }\n}\n\nconst loadMore = () => {\n  loading.value = true\n  setTimeout(() => {\n    displayCount.value += 12\n    loading.value = false\n  }, 500)\n}\n\n// Reset display count when category changes\nwatch(activeCategory, () => {\n  displayCount.value = 20\n})\n\n// Keyboard navigation for lightbox\nonMounted(() => {\n  const handleKeydown = (e) => {\n    if (!lightboxOpen.value) return\n    \n    if (e.key === 'Escape') {\n      closeLightbox()\n    } else if (e.key === 'ArrowRight') {\n      nextImage()\n    } else if (e.key === 'ArrowLeft') {\n      previousImage()\n    }\n  }\n  \n  document.addEventListener('keydown', handleKeydown)\n  \n  onUnmounted(() => {\n    document.removeEventListener('keydown', handleKeydown)\n  })\n})\n</script>\n"],"version":3}